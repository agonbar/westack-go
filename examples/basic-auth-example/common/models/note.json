{
  "name": "note",
  "base": "PersistedModel",
  "public": true,
  "properties": {
    "email": {
      "type": "string",
      "required": true
    }
  },
  "relations": {
    "roles": {
      "type": "hasAndBelongsToMany",
      "model": "Role",
      "modelThrough": "RoleMapping",
      "foreignKey": "userId",
      "keyThrough": "roleId"
    }
  },
  "hidden": ["password"]
}